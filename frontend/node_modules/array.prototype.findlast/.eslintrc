{
	"root": true,

	"extends": "@ljharb",

	"rules": {
		"array-bracket-newline": 0,
		"id-length": 0,
		"max-statements-per-line": [2, { "max": 2 ***REMOVED***],
		"new-cap": [2, {
			"capIsNewExceptions": [
				"GetIntrinsic",
				"Call",
				"Get",
				"IsCallable",
				"LengthOfArrayLike",
				"RequireObjectCoercible",
				"ToBoolean",
				"ToObject",
				"ToString",
			],
		***REMOVED***],
		"no-magic-numbers": 0,
	***REMOVED***,

	"overrides": [
		{
			"files": "test/**",
			"rules": {
				"func-name-matching": 0,
				"max-lines-per-function": 0,
				"no-invalid-this": 1,
				"strict": 1,
			***REMOVED***,
		***REMOVED***,
	],
***REMOVED***
